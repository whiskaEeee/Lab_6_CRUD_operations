<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Lab_6_CRUD_operations</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Lab_6_CRUD_operations.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3 px-5">
            <div class="container-fluid">
                <img style="height: 40px; width: 40px;" src="~/images/z.svg">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item px-3">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Store" asp-action="shop">Магазин</a>
                        </li>
                        <li class="nav-item px-3">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Department" asp-action="department">Отдел</a>
                        </li>
                        <li class="nav-item px-3">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Employee" asp-action="employee">Сотрудник</a>
                        </li>
                        <li class="nav-item px-3 ">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Товар</a>
                        </li>
                        <li class="nav-item px-3 ">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Договоры</a>
                        </li>
                        <li class="nav-item px-3 ">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Поставщик</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    


    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Lab_6_CRUD_operations - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
